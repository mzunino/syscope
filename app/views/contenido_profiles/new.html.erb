<h1>New contenido_profile</h1>

<% form_for(@contenido_profile) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :contenido_id %><br />
    <%= f.collection_select :contenido_id, Contenido.find(:all), :id, :descripcion, :prompt => "Seleccione un Contenido" %>
  </p>
  <p>
    <%= f.label :profile_id %><br />
    <%= f.collection_select :profile_id, Profile.find(:all), :id, :descripcion, :prompt => "Seleccione un Profile" %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', contenido_profiles_path %>
